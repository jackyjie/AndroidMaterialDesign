<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.materialdesign">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ToolBar.ToolbarActivity" />
        <activity android:name=".ToolBar.ToolBarMenuActivity" />
        <activity android:name=".ToolBar.ToolbarSlideActivity" />
        <activity android:name=".ToolBar.NavigationViewActivity" />
        <activity android:name=".ToolBar.FloatActivity" />
        <activity android:name=".ToolBar.CollapsingToolbarActivity"
            android:theme="@style/FruitItemTheme"></activity>
    </application>

</manifest>